{
  "name": "newtab",
  "version": "0.5.1",
  "main": "index.js",
  "repository": "https://github.com/mkusaka/open-new-window-chrome-extension.git",
  "author": "mkusaka <hinoshita1992@gmail.com>",
  "license": "MIT",
  "packageManager": "pnpm@9.15.9",
  "dependencies": {},
  "devDependencies": {
    "@crxjs/vite-plugin": "2.2.0",
    "@types/chrome": "0.0.270",
    "oxlint": "1.16.0",
    "prettier": "3.6.2",
    "typescript": "5.9.2",
    "vite": "7.1.5"
  },
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "oxlint . --import-plugin --tsconfig ./tsconfig.json",
    "lint:fix": "oxlint . --import-plugin --tsconfig ./tsconfig.json --fix",
    "lint:ci": "oxlint . --import-plugin --tsconfig ./tsconfig.json --deny-warnings",
    "package": "rm -f package.zip && pnpm build && cd dist && zip -r ../package.zip ."
  }
}
